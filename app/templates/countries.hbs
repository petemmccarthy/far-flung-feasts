{{#each model as |country|}}
  <div class="col-md-4 mt50">
    <figure>
        <img class="country-image" src="/assets/images/{{country.name}}.png" alt="{{country.name}}">
      <figcaption class="text-center mt20">
        <h4>{{country.name}}</h4>
      </figcaption>
    </figure>
  </div>
{{/each}}
